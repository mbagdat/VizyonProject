<div class="container">

    <div class="modal fade" id="myAciklamaModal" role="dialog">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    @*<h4 class="modal-title"></h4>*@
                </div>

                <div class="modal-body">
                    <form class="form-group col-md-12">
                        <div class="col-md-6">
                            <label class="control-label">Oluşturulma Tarihi</label>
                            <input id="olusturmaTarihi" name="olusturmaTarihi" type="date" value="" />
                        </div>

                        <div class="col-md-6">
                            <label for="aciklama">Güncelleme Tarihi</label>
                            <input id="guncelleTarihi" name="guncelleTarihi" type="date" value="" /><br />
                        </div>

                        <div class="col-md-6">
                            <label for="aciklama">Onay Tarihi</label>
                            <input id="onayTarihi" name="onayTarihi" type="date" value="" />
                        </div>

                        <div class="col-md-6">
                            <label for="aciklama">Puan Ver</label>
                            <input id="puanVer" name="puanVer" type="number" value="1" min="1" max="10" />
                        </div>

                        <div class="col-md-12">
                            <label for="aciklama">Açıklama</label>
                            <textarea style="resize: none;" class="form-control" cols="20" id="Description" name="Description" rows="2"></textarea>
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="submit" id="sektorUpdate" class="btn btn-primary">Save</button>
                    <button type="button" class="btn btn-outline-danger" data-dismiss="modal">Close</button>
                </div>


            </div>
        </div>
    </div>
</div>